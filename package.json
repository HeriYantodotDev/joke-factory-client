    "test:coverage": "jest --verbose --coverage",
    "test:watch": "jest --verbose --watchAll",
    "test:one": "jest --testPathPattern=SignUp.Component.test.ts",
    "test:one:watch": "jest --testPathPattern=SignUp.Component.test.ts --watchAll"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "devDependencies": {
