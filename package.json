{
  "name": "react-ts-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.cjs,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.cjs,.ts,.tsx . --fix",
    "typecheck": "tsc --noEmit",
